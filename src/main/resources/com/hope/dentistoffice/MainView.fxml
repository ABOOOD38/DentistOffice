<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="224.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hope.dentistoffice.controllers.MainController">
    <VBox layoutX="63.0" layoutY="14.0" prefHeight="200.0" prefWidth="210.0">
        <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Welcome again Admin " textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="20.0" left="25.0" top="10.0" />
            </VBox.margin>
            <font>
               <Font name="System Bold" size="13.0" />
            </font></Label>
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>

                <Button fx:id="addDoctorBtn" mnemonicParsing="false" onAction="#onCLickAddDoctorBtn" prefHeight="24.0" prefWidth="96.0" text="AddDoctor">
                    <font>
                        <Font name="Ubuntu" size="13.0" />
                    </font>
                </Button>

                <Button fx:id="addPatientBtn" mnemonicParsing="false" onAction="#onClickAddPatientBtn" prefHeight="24.0" prefWidth="96.0" text="AddPatient" GridPane.columnIndex="1" />
            </GridPane>
        </children>
    </VBox>
</AnchorPane>
